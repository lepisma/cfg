;; Tartarus Macropad mapping

(defcfg
  input (device-file "/dev/input/by-id/usb-Razer_Razer_Tartarus_V2-event-kbd")
  output (uinput-sink "Tartarus KMonad")
  cmp-seq ralt
  fallthrough false
  allow-cmd true)

(defsrc
  1 2 3 4 5
  tab q w e r
  caps a s d f
  lsft z x c
  spc)

(deflayer default
  XX XX 7 8 9
  XX XX 4 5 6
  home end 1 2 3
  C-S-tab C-tab XX XX
  spc)
